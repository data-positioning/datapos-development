{
    "name": "@datapos/datapos-development",
    "version": "0.3.349",
    "description": "A collection of utilities for managing Data Positioning projects.",
    "license": "MIT",
    "author": "Jonathan Terrell <terrell.jm@gmail.com>",
    "private": false,
    "type": "module",
    "homepage": "https://www.datapos.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/data-positioning/datapos-development.git"
    },
    "bugs": {
        "url": "https://github.com/data-positioning/datapos-development/issues"
    },
    "module": "./dist/datapos-development.es.js",
    "types": "./dist/types/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/datapos-development.es.js",
            "types": "./dist/types/src/index.d.ts"
        },
        "./prettierrc": "./.prettierrc.json",
        "./license-report-config": "./licenses/license-report-config.json"
    },
    "files": [
        ".editorconfig",
        ".prettierrc.json",
        "dist",
        "licenses/license-report-config.json"
    ],
    "dependencies": {
        "@datapos/datapos-shared": "^0.3.298",
        "acorn": "^8.15.0",
        "acorn-typescript": "^1.4.13",
        "acorn-walk": "^8.3.4",
        "dotenv": "^17.2.3",
        "nanoid": "^5.1.6",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@datapos/eslint-config-datapos": "^1.0.18",
        "@types/eslint-plugin-security": "^3.0.0",
        "@types/node": "^24.10.2",
        "@typescript-eslint/eslint-plugin": "^8.49.0",
        "@typescript-eslint/parser": "^8.49.0",
        "eslint": "^9.39.1",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.5",
        "eslint-plugin-unicorn": "^62.0.0",
        "jiti": "^2.6.1",
        "license-downloader": "^1.3.3",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "license-report-recursive": "^6.8.2",
        "npm-check-updates": "^19.1.2",
        "owasp-dependency-check": "^1.0.0",
        "prettier": "^3.7.4",
        "rollup-plugin-visualizer": "^6.0.5",
        "sonda": "^0.10.1",
        "ts-to-zod": "^5.1.0",
        "type-fest": "^5.3.1",
        "typescript": "^5.9.3",
        "vite": "^7.2.7",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.15"
    },
    "scripts": {
        "audit": "op run --env-file=.env -- node -e \"import('./dist/datapos-development.es.js').then(m => m.auditDependencies())\"",
        "build": "node -e \"import('./dist/datapos-development.es.js').then(m => m.buildProject())\"",
        "check": "node -e \"import('./dist/datapos-development.es.js').then(m => m.checkDependencies())\"",
        "document": "op run --env-file=.env -- node -e \"import('./dist/datapos-development.es.js').then(m => m.documentDependencies(['BSD-2-Clause', 'MIT']))\"",
        "format": "node -e \"import('./dist/datapos-development.es.js').then(m => m.formatCode())\"",
        "lint": "node -e \"import('./dist/datapos-development.es.js').then(m => m.lintCode())\"",
        "release": "op run --env-file=.env -- node -e \"import('./dist/datapos-development.es.js').then(m => m.releaseProject())\"",
        "sync": "node -e \"import('./dist/datapos-development.es.js').then(m => m.syncProjectWithGitHub())\"",
        "test": "node -e \"import('./dist/datapos-development.es.js').then(m => m.testProject())\"",
        "update": "node -e \"import('./dist/datapos-development.es.js').then(m => m.updateDataPosDependencies(['shared']))\"",
        "___ts-to-zod": "npx ts-to-zod src/schemaTypes.ts src/schemaZOD.ts"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=11.0.0"
    }
}